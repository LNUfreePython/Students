class Solution:
    def isIsomorphic(self, s: str, t: str) ->bool:
        i = 0
        h1,h2 = {},{}
        length = len(s)
        while i < length:
            if s[i] not in h1: h1 [s[i]] = t[i]
            if t[i] not in h2: h2[t[i]] = s[i]
            if h1[s[i]] != t[i] or h2[t[i]] != s[i]:
                return False
            i += 1
        return True        